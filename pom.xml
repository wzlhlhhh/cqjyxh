    </dependency>
	<!-- MD5加密 -->
	<dependency>
	 <groupId>commons-codec</groupId>
	 <artifactId>commons-codec</artifactId>
	 <version>1.10</version>
	</dependency>
  </dependencies>
  <build>
    <finalName>vone</finalName>
  </build>
</project>
